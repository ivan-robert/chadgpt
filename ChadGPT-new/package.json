{
  "name": "chadgpt",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "patch-package",
    "updateTranslations": "bun scripts/updateTranslations.ts && npx prettier --write src/app/translations/**/*.json",
    "checkTranslations": "bun scripts/checkTranslations.ts",
    "start": "ENV=development expo start",
    "test": "jest",
    "lint": "yarn eslint . --ext .jsx,.ts,.tsx --max-warnings 0",
    "android": "expo run:android",
    "test:all": "yarn eslint . --ext .jsx,.ts,.tsx --max-warnings 0 && tsc && jest",
    "icons:create": "svgr --native --typescript -d ./src/shared/View/icons assets/icons --ignore-existing --no-index --template src/shared/View/services/svg/templateTsx.js && yarn lint:fix --quiet",
    "ios": "expo run:ios",
    "count-lines": "git ls-files | grep 'src' | grep -E '.ts$|.tsx$' | grep -vE '.ttf$|.otf$' | xargs wc -l",
    "deploy:android:development": "eas build --platform android --profile development",
    "deploy:android:staging": "eas build --platform android --profile staging",
    "deploy:android:production": "eas build --platform android --profile production --auto-submit",
    "deploy:ios:staging": "eas build --platform ios --profile staging",
    "deploy:ios:production": "eas build --platform ios --profile production --auto-submit",
    "deploy:production": "eas build --platform all --profile production --auto-submit",
    "submit:ios": "eas submit -p ios --latest --profile production",
    "submit:android": "eas submit -p android --profile production",
    "show-debug-key": "keytool -list -v -keystore android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "@clerk/clerk-expo": "^2.2.14",
    "@emotion/native": "11.11.0",
    "@emotion/react": "11.11.4",
    "@eslint/js": "^9.3.0",
    "@hookform/resolvers": "3.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@rneui/base": "4.0.0-rc.8",
    "@rneui/themed": "4.0.0-rc.8",
    "@shopify/flash-list": "1.6.4",
    "@supabase/supabase-js": "2.43.2",
